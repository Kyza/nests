{
	"name": "Nests DevTools",
	"description": "A state inspector for state management libraries.",
	"version": "1.0.0",
	"manifest_version": 3,
	"devtools_page": "http://localhost:3001/",
	// "devtools_page": "devtools/dist/index.html",
	// "background": {
	// 	"service_worker": "background.js"
	// },
	"permissions": ["scripting", "tabs"],
	"host_permissions": ["*://*/*"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"run_at": "document_start"
		}
	]
}
