{
	"compilerOptions": {
		"outDir": "./test",
		"target": "ES6",
		"module": "CommonJS",
		"lib": ["ES6", "DOM"],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"noUnusedParameters": false,
		"noUnusedLocals": false,
		"isolatedModules": true,
		"removeComments": true
	},
	"include": ["test.ts", "src"],
	"exclude": ["node_modules"]
}
