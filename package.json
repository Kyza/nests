{
	"name": "nests",
	"version": "3.0.0-alpha.0",
	"description": "Fast and easy state storage with a lot of control.",
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"types": "./types/index.d.js",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"default": "./cjs/index.js",
			"module": "./esm/index.js",
			"import": "./mjs/index.mjs",
			"types": "./types/index.d.ts"
		},
		"./react": {
			"default": "./cjs/react/index.js",
			"module": "./esm/react/index.js",
			"import": "./mjs/react/index.mjs",
			"types": "./types/react/index.d.ts"
		},
		"./solid-js": {
			"default": "./cjs/solid-js/index.js",
			"module": "./esm/solid-js/index.js",
			"import": "./mjs/solid-js/index.mjs",
			"types": "./types/solid-js/index.d.ts"
		}
	},
	"sideEffects": false,
	"files": [
		"**"
	],
	"scripts": {
		"build": "npx zx ./scripts/build.mjs",
		"dev": "npx swc ./src/ -d ./dev/ --watch",
		"pub": "cd ./lib && npm publish && cd ..",
		"pub-alpha": "cd ./lib && npm publish --tag alpha && cd ..",
		"pub-beta": "cd ./lib && npm publish --tag beta && cd ..",
		"test": "tsc --project tsconfig.test.json && node ./test/test.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kyza/nests"
	},
	"bugs": "https://github.com/Kyza/nests/issues",
	"keywords": [
		"flux",
		"redux",
		"nests",
		"state",
		"store",
		"simple",
		"predictable",
		"easy",
		"deep",
		"react",
		"immer",
		"solid-js"
	],
	"author": "Kyza",
	"license": "MIT",
	"devDependencies": {
		"@swc/cli": "^0.1.49",
		"@swc/core": "^1.2.84",
		"@types/node": "^16.7.13",
		"@types/react": "^17.0.19",
		"@types/react-dom": "^17.0.9",
		"chokidar": "^3.5.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"renamer": "^4.0.0",
		"solid-js": "^1.1.5",
		"typescript": "^4.4.2",
		"zx": "^4.2.0"
	},
	"peerDependencies": {
		"react": ">=16.8",
		"solid-js": ">=1.0.0"
	},
	"peerDependencimjseta": {
		"react": {
			"optional": true
		},
		"solid-js": {
			"optional": true
		}
	}
}
