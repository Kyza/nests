{
	"name": "nests",
	"version": "4.0.0-alpha.11",
	"description": "Fast and easy state storage with a lot of control.",
	"main": "./index.js",
	"module": "./esm/index.js",
	"types": "./index.d.js",
	"sideEffects": false,
	"files": [
		"**"
	],
	"scripts": {
		"build": "npx zx ./scripts/build.mjs && npx minified-size ./esm/**/*.js",
		"dev": "tsc -p ./tsconfig.dev.json -w",
		"pub": "cd dist && npm publish",
		"pub-alpha": "cd dist && npm publish --tag alpha",
		"pub-beta": "cd dist && npm publish --tag beta",
		"test": "tsc --project tsconfig.test.json && node ./test/test.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kyza/nests"
	},
	"bugs": "https://github.com/Kyza/nests/issues",
	"keywords": [
		"flux",
		"redux",
		"nests",
		"state",
		"store",
		"simple",
		"predictable",
		"easy",
		"deep",
		"react",
		"immer",
		"solid-js",
		"immutable"
	],
	"author": "Kyza",
	"license": "MIT",
	"devDependencies": {
		"@swc/cli": "^0.1.49",
		"@swc/core": "^1.2.84",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.7.13",
		"@types/react": "^17.0.19",
		"@types/react-dom": "^17.0.9",
		"chokidar": "^3.5.2",
		"fs-extra": "^10.0.0",
		"jest": "^27.3.1",
		"minified-size": "^2.3.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"renamer": "^4.0.0",
		"solid-js": "^1.1.5",
		"typescript": "^4.5.4",
		"zx": "^4.2.0"
	},
	"peerDependencies": {
		"react": ">=16.8",
		"solid-js": ">=1.3.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		},
		"solid-js": {
			"optional": true
		}
	}
}
